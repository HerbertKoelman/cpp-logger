/* autoconf section ----------------------------------------------------------------------*/

#ifndef cpp_logger_config_h
#define cpp_logger_config_h

#undef CPP_LOGGER_VERSION


/* Define to 1 if you have the `pthread' library (-lpthread). */
#undef HAVE_LIBPTHREAD

#undef HAVE_LIBCPP_PTHREAD

/* Defined if compiler support C++11 */
#undef HAVE_CPP11_MUTEX
#undef HAVE_CPP11_CHRONO
#undef HAVE_CPP11_THREAD

/*
 * Define to `__inline__' or `__inline' if that's what the C compiler
 * calls it, or to nothing if 'inline' is not supported under any name. 
 */
#ifndef __cplusplus
#undef inline
#endif

#endif /* cpp_logger_config_h */
